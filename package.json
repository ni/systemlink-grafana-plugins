{
  "name": "systemlink-grafana-plugins",
  "version": "1.8.0",
  "description": "Plugins for visualizing data from SystemLink services",
  "private": true,
  "scripts": {
    "start": "npm run server && npm run dev",
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "test:template": "npx plop datasource Test && npx tsc --noEmit && rimraf src/datasources/test",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "npm run lint -- --fix",
    "e2e": "npm exec cypress install && npm exec grafana-e2e run",
    "e2e:update": "npm exec cypress install && npm exec grafana-e2e run --update-screenshots",
    "server": "docker-compose up --build -d",
    "generate": "plop",
    "release": "semantic-release"
  },
  "author": "NI",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@grafana/e2e": "9.5.5",
    "@grafana/e2e-selectors": "9.5.5",
    "@grafana/eslint-config": "^6.0.0",
    "@grafana/tsconfig": "^1.2.0-rc1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@swc/core": "^1.3.62",
    "@swc/helpers": "^0.5.0",
    "@swc/jest": "^0.2.26",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.194",
    "@types/node": "^18.15.11",
    "@types/plotly.js-basic-dist-min": "^2.12.1",
    "@types/react-plotly.js": "^2.6.0",
    "@types/react-router-dom": "^5.3.3",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "eslint": "^8.43.0",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "glob": "^10.2.7",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-mock-extended": "^3.0.4",
    "plop": "^3.1.2",
    "prettier": "^2.8.7",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.63.2",
    "sass-loader": "13.3.1",
    "semantic-release": "^21.0.7",
    "style-loader": "3.3.3",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "4.8.4",
    "webpack": "^5.86.0",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@emotion/css": "^11.1.3",
    "@grafana/data": "9.5.5",
    "@grafana/runtime": "9.5.5",
    "@grafana/ui": "9.5.5",
    "@isaacs/ttlcache": "^1.4.1",
    "@types/file-saver": "^2.0.5",
    "ajv": "^8.12.0",
    "file-saver": "^2.0.5",
    "plotly.js-basic-dist-min": "^2.24.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-plotly.js": "^2.6.0",
    "react-router-dom": "^5.2.0",
    "rxjs": "7.8.0",
    "smart-webcomponents-react": "^12.0.0",
    "tslib": "2.5.3"
  }
}
